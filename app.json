{
  "expo": {
    "name": "meibu",
    "slug": "meibu",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "meibu",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.robin404.meibu",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.resi.meibu",
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Erlaube MeiBu FaceID zu benutzen."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4b422b19-0b20-4020-90fc-39831db57258"
      }
    },
    "owner": "ninia963"
  }
}
